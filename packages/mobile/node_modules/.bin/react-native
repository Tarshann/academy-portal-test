#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-native@0.73.11_@babel_cd420731e1d33d89ba4e038abd3c94b7/node_modules/react-native/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-native@0.73.11_@babel_cd420731e1d33d89ba4e038abd3c94b7/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-native@0.73.11_@babel_cd420731e1d33d89ba4e038abd3c94b7/node_modules/react-native/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-native@0.73.11_@babel_cd420731e1d33d89ba4e038abd3c94b7/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
