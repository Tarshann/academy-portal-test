#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules/react-scripts/bin/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules/react-scripts/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules/react-scripts/bin/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules/react-scripts/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/react-scripts@3.4.4_eslint@_1821028aef270efb571ed745b56bad46/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
