#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules/vite/bin/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules/vite/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules/vite/bin/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules/vite/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/vite@6.3.1_@types+node@22.1_2febfe9bb7d7a9397b291564b2364547/node_modules:/mnt/c/Users/user/academy-portal-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../vite/bin/vite.js" "$@"
fi
